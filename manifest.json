{
  "name": "Website BGM",
  "description": "An helpful chrome extension that plays a unique welcome song for every website you visit",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "css": ["style.css"],
      "js": ["jquery.js","script.js"]
    }
  ],
  "manifest_version": 2,
  "icons":{
  	"16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
